<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ekomp.Controllers.StatusOrdersController">
   <children>
      <Label layoutX="204.0" layoutY="30.0" text="Zmień status">
         <font>
            <Font name="System Bold" size="32.0" />
         </font>
      </Label>
      <ListView fx:id="ordersList" layoutX="204.0" layoutY="98.0" prefHeight="151.0" prefWidth="193.0" />
      <Button fx:id="selectOrderBt" layoutX="268.0" layoutY="257.0" mnemonicParsing="false" onAction="#switchToChangeStatusList" text="Wybierz" />
      <Label fx:id="info" layoutX="211.0" layoutY="72.0" text="Wybierz zamówienie z listy:" />
      <Label fx:id="or" layoutX="293.0" layoutY="293.0" text="lub:" />
      <TextField fx:id="orderNr" layoutX="147.0" layoutY="318.0" prefHeight="31.0" prefWidth="193.0" promptText="Wprowadź nr zamówienia" />
      <Button fx:id="searchOrderBt" layoutX="350.0" layoutY="318.0" mnemonicParsing="false" onAction="#switchToChangeStatusField" prefHeight="31.0" prefWidth="113.0" text="Szukaj" />
      <Button layoutX="263.0" layoutY="357.0" mnemonicParsing="false" onAction="#switchToMain" text="Zakończ" />
   </children>
</AnchorPane>
